<?xml version="1.0"?>
<tryton>
    <data>
        <!-- ############# -->
        <!-- # Root Menu # -->
        <!-- ############# -->
        <menuitem name="Ouverture" id="menu_ouverture" sequence="1"/>
        <record model="ir.ui.view" id="ouverture_view_form">
            <field name="model">ouverture.ouverture</field>
            <field name="type">form</field>
            <field name="name">ouverture_form</field>
        </record>
        <record model="ir.ui.view" id="ouverture_view_list">
            <field name="model">ouverture.ouverture</field>
            <field name="type">tree</field>
            <field name="name">ouverture_list</field>
        </record>
        <record model="ir.action.act_window" id="act_ouverture">
            <field name="name">Ouverture</field>
            <field name="res_model">ouverture.ouverture</field>
        </record>
        <record model="ir.action.act_window.view" id="act_ouverture_view_list">
            <field name="sequence" eval="10"/>
            <field name="view" ref="ouverture_view_list"/>
            <field name="act_window" ref="act_ouverture"/>
        </record>
        <record model="ir.action.act_window.view" id="act_ouverture_view_form">
            <field name="sequence" eval="20"/>
            <field name="view" ref="ouverture_view_form"/>
            <field name="act_window" ref="act_ouverture"/>
        </record>
